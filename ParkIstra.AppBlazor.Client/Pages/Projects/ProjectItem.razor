@page "/projects/project"
@layout MainLayoutNav

<div class="container">
    <h1>@Project.Name</h1>
    <p>@Project.Description</p>

    <div class="row">
        @foreach (var imageUrl in Project.Images)
        {
            <div class="col-md-4">
                <img src="@imageUrl" class="img-fluid" alt="Project Image">
            </div>
        }
    </div>
</div>
